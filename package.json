  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "vitest",
    "prepare": "husky install",
    "commit": "cz",
    "format": "prettier --write .",
    "lint": "eslint '**/*.{js,cjs,mjs}'",
    "lint:fix": "eslint '**/*.{js,cjs,mjs}' --fix",
    "lint:all": "eslint '**/*.{js,cjs,mjs}' --fix && prettier --write .",
    "dev:api": "cd server && npm run dev",
